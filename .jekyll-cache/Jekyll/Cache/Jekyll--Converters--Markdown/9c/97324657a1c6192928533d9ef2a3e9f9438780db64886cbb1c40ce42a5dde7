// DFSë¥¼ í™œìš©í•œ ë¯¸ë¡œì°¾ê¸° í•¨ìˆ˜
Graph.prototype.findMaze = function (start, goal, _visited = {}) {
  let finish = false; // goalì— ë„ì°©í–ˆëŠ”ê°€ë¥¼ íŒì •
  const visited = _visited; // ë°©ë¬¸ ì—¬ë¶€ë¥¼ ê¸°ë¡</p>

<p>// í•´ë‹¹ ì •ì ì„ ë°©ë¬¸í•œë‹¤
  visited[start] = true;
  process.stdout.write(<code class="language-plaintext highlighter-rouge">${start} </code>);</p>

<p>// goalì— ë„ì°©í•œ ê²½ìš°
  if (start === goal) finish = true;</p>

<p>for (const adjacent in this.edges[start]) { // í•´ë‹¹ ì •ì ì˜ ì¸ì ‘ ì •ì ì„ íƒìƒ‰í•œë‹¤
    if (Object.hasOwnProperty.call(this.edges[start], adjacent)) {
      if (!visited[adjacent] &amp;&amp; !finish) { // ë°©ë¬¸í•˜ì§€ ì•Šì•˜ê³ , goalì— ë„ì°©í•˜ì§€ ì•Šì•˜ë‹¤ë©´
        finish = this.findMaze(adjacent, goal, visited); // í•´ë‹¹ ì •ì ìœ¼ë¡œë¶€í„° ì¬ê·€ í˜¸ì¶œ
      }
    }
  }
  return finish;
};
graph.findMaze(start, goal); // 2, 9
